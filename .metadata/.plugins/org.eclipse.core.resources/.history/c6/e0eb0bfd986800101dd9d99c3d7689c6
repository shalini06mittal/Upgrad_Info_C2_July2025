package interfaces;
// default methods
interface Message{
	public void sendMessage(String msg);
	public default void notifyUser() {
		
	}
}

// class has to provide the implementation of all the abstarct methods
// or else declare the class as abstract
class A implements Message{

	@Override
	public void sendMessage(String msg) {
		// TODO Auto-generated method stub
		
	}

	
}

class B implements Message{

	@Override
	public void sendMessage(String msg) {
		// TODO Auto-generated method stub
		
	}
	
}


public class TestInterfaces {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

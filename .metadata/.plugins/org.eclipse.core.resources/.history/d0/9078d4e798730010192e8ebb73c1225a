package threads;

/**
 * 1. Extend Thread class
 * 2. Override the run() : task that a thread will perform
 * 3. create object of your thread class
 * 4. call start
 */
public class EvenThread  extends Thread{
	
	@Override
	public void run() {
		
		System.out.println(Thread.currentThread().getName()+ " in run");
		for (int i = 0; i <=5; i++) {
				System.out.println();
		}
	}

}

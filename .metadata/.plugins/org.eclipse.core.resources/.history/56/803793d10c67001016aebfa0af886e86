package demo;
/**
 * 1. abstract is a keyword
 * 2. classes and method ONLY
 * 3. abstract classess cannot be instantiated
 * 4. abstract methods cannot have implementation
 * 5. abstract classes may or may not have abstract methods but if a class has
 * even one abstract method, you should declare the class as abstract
 */
public class Account {
	
	private int accid;
	private String name;
	private double balance;
	
	public Account() {
		System.out.println("Account def constructor");
		// TODO Auto-generated constructor stub
	}

	public Account(int accid, String name, double balance) {
		System.out.println("Account param constructor");
		this.accid = accid;
		this.name = name;
		this.balance = balance;
	}
	public Account(int accid, String name) {
		System.out.println("Account param constructor");
		this.accid = accid;
		this.name = name;
		
	}

	public int getAccid() {
		return accid;
	}

	public void setAccid(int accid) {
		this.accid = accid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getBalance() {
		return balance;
	}

	public void setBalance(double balance) {
		this.balance = balance;
	}

	public abstract void calculateInterestRat();
	@Override
	public String toString() {
		return "Account [accid=" + accid + ", name=" + name + ", balance=" + balance + "]";
	}
	

}

package demo;

public class Employee extends Object{

	
	private int id;
	private String name;
	
	public Employee() {
		System.out.println("employee def constructor");
	}

	public Employee(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	//method overriding? -> poluymorphism
}
